    "@types/react": "17.0.43",
    "eslint": "7.32.0",
    "husky": "7.0.4",
    "lint-staged": "11.3.0-beta.2",
    "prettier": "2.6.1",
    "prettier-plugin-prisma": "3.11.0",
    "pretty-quick": "3.1.3",
    "preview-email": "3.0.5",
