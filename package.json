    "@hookform/resolvers": "2.8.8",
    "@pdftron/webviewer": "8.3.3",
    "@prisma/client": "3.11.1",
    "blitz": "0.45.3",
    "chance": "1.1.8",
    "devextreme": "21.2.6",
    "devextreme-react": "21.2.6",
    "dhx-suite": "7.3.1",
